{
  "name": "popover-project",
  "version": "1.0.0",
  "description": "Popover widget with Webpack, Appveyor, and tests",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --config src/webpack.config.js --mode production",
    "test": "jest",
    "deploy": "gh-pages -d dist",
    "start": "webpack serve --config src/webpack.config.js --open"

  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFiles": [
      "<rootDir>/src/test/setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!@babel/runtime)"
    ]
  },
  "keywords": [
    "popover",
    "javascript",
    "webpack",
    "appveyor",
    "github pages"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.11.0",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mini-css-extract-plugin": "^2.9.2",
    "puppeteer": "^22.3.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.1"
  },
  "dependencies": {
    "core-js": "^3.36.0"
  }
}
